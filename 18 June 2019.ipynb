    "def hightReminder(n):\n",
    "    hr =0\n",
    "    v =n\n",
    "    for i in range(n-1, n//2,-1):\n",
    "        r= n%i\n",
    "        if r>hr:\n",
    "            v=i\n",
    "    print(v)\n",
    "    return \n",
    "hightReminder(5)\n",
    "        \n",
